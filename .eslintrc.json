{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:jest/recommended", "plugin:jest/style", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["jest", "prettier"],
  "rules": {
    "camelcase": "off",
    "no-console": "off"
  }
}
